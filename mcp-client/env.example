# Example environment variables for MCP Client
# Copy this file to .env and modify as needed

# Server configuration
MCP_SERVER_COMMAND=python
MCP_SERVER_ARGS=examples/simple_server.py
MCP_DEBUG=false

# Transport settings
MCP_TRANSPORT_TYPE=stdio
MCP_TIMEOUT=30

# Optional: Custom environment variables for the server
# API_KEY=your-api-key-here
# DATABASE_URL=your-database-url-here 